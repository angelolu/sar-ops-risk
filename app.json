{
  "expo": {
    "name": "OpsRisk",
    "slug": "OpsRisk",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "opsrisk",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash_light.png",
        "resizeMode": "contain",
        "backgroundColor": "#F7F9FF",
        "dark": {
          "image": "./assets/splash_dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#101418"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "monochromeImage": "./assets/adaptive-icon-mono.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.ca_sar.risk",
      "splash": {
        "image": "./assets/splash_light.png",
        "resizeMode": "contain",
        "backgroundColor": "#F7F9FF",
        "dark": {
          "image": "./assets/splash_dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#101418"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "single",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fa9a7646-28ad-47c1-a926-8ed2f29706cd"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fa9a7646-28ad-47c1-a926-8ed2f29706cd"
    }
  }
}